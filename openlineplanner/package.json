{
  "name": "openlineplanner",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.0.8",
    "apexcharts": "^3.45.1",
    "geographiclib-geodesic": "^2.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "mapbox-gl": "^3.1.2",
    "pinia": "^2.1.7",
    "sass": "^1.70.0",
    "v-onboarding": "^2.5.1",
    "vue": "^3.4.15",
    "vue-color-kit": "^1.0.5",
    "vue-material-design-icons": "^5.0.0",
    "vue-matomo": "^4.1.0",
    "vue-router": "^4.2.5",
    "vue-slider-component": "^4.1.0-beta.2",
    "vue3-apexcharts": "^1.4.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "^20.11.16",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,scss,css}": [
      "prettier --write"
    ]
  }
}
